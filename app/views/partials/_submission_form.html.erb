<%= form_for submission do |f| %>
  <% if not submission.errors.empty? %>
    <h2>There are the following errors:</h2>
    <ul>
      <% submission.errors.each do |e| %>
        <li><%= e %></li>
      <% end %>
    </ul>
  <% end %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.submit 'Submit' %>
<% end %>
